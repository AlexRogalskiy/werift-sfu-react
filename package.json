{
  "name": "uyem",
  "version": "0.1.27",
  "repository": "https://github.com/kolserdav/uyem.git",
  "description": "Selective Forwarding Unit (SFU) web application, server side of which written on Node and client side on ReactJS",
  "keywords": [
    "node",
    "react",
    "webrtc",
    "p2p",
    "peer"
  ],
  "homepage": "https://uyem.ru",
  "author": {
    "name": "Sergey Kolmiller",
    "email": "uyem.ru@gmail.com"
  },
  "maintainers": [
    "Sergey Kolmiller <uyem.ru@gmail.com>"
  ],
  "contributors": [
    "Sergey Kolmiller <uyem.ru@gmail.com>"
  ],
  "bugs": "https://github.com/kolserdav/uyem/issues",
  "license": "BSD-2-Clause",
  "main": "packages/client/dist/Main.js",
  "types": "packages/client/dist/Main.d.ts",
  "files": [
    "packages/client/dist",
    "packages/server/dist",
    "bin",
    "server.js",
    "server.d.ts",
    "scripts",
    "THIRD_PARTY_LICENSES.md",
    "LICENSE"
  ],
  "os": [
    "linux"
  ],
  "bin": {
    "uyem": "bin/uyem.js"
  },
  "scripts": {
    "build:server": "cd packages/server && npm run build",
    "prebuild:client": "cd packages/client && npm run build",
    "build:client": "cd packages/client && npm run compile",
    "build": "npm run build:server && npm run build:client",
    "copy-interfaces": "node scripts/create-interfaces-symlink.js",
    "stun": "cd packages/server && node-stun-server",
    "preinstall": "npm run copy-interfaces",
    "dev:server": "cd packages/server && npm run dev",
    "dev:install": "cd packages/server/node-webrtc && SKIP_DOWNLOAD=true npm install",
    "dev:client": "cd packages/client && npm start",
    "start": "cd packages/server && npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "test": "node tests/rooms.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "clsx": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^8.3.2",
    "wrtc": "^0.4.7",
    "ws": "^8.7.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.39",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3"
  },
  "optionalDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/puppeteer": "^5.4.6",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "airbnb": "^0.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.2.1",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "puppeteer": "^14.4.1",
    "react-scripts": "5.0.1",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "fhead": {
    "root": "packages",
    "repository": "https://github.com/kolserdav/uyem.git",
    "patterns": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "node-webrtc",
      "dist",
      "build"
    ],
    "name": "Sergey Kolmiller",
    "email": "uyem.ru@gmail.com",
    "license": "BSD-2-Clause",
    "licenseText": "Binary distributions of this software include 'wrtc' and other third-party libraries.",
    "copyright": "kolserdav, All rights reserved (c)",
    "renewAll": false
  }
}
