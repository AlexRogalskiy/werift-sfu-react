{
  "name": "uyem",
  "version": "1.0.12",
  "repository": "https://github.com/kolserdav/uyem.git",
  "description": "Selective Forwarding Unit (SFU) server side of which written on Node and client side on ReactJS",
  "keywords": [
    "node",
    "react",
    "webrtc",
    "p2p",
    "peer"
  ],
  "author": {
    "name": "Sergey Kolmiller",
    "email": "uyem.ru@gmail.com"
  },
  "bugs": "https://github.com/kolserdav/uyem/issues",
  "license": "BSD-2-Clause",
  "files": [
    "packages/server/dist",
    "packages/client/dist",
    "bin",
    "client.js",
    "server.js",
    "client.d.ts",
    "server.d.ts",
    "index.js",
    "index.d.ts",
    "scripts",
    "THIRD_PARTY_LICENSES.md",
    "LICENSE"
  ],
  "os": [
    "linux"
  ],
  "bin": {
    "uyem": "bin/uyem.js"
  },
  "main": "index.js",
  "scripts": {
    "build:server": "cd packages/server && npm run build",
    "build:client": "cd packages/client && npm run compile",
    "build": "npm run build:server && npm run build:client",
    "copy-interfaces": "node scripts/create-interfaces-symlink.js",
    "preinstall": "npm run copy-interfaces",
    "dev:server": "cd packages/server && npm run dev",
    "dev:install": "cd packages/server/node-webrtc && SKIP_DOWNLOAD=true npm install",
    "dev:client": "cd packages/client && npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "bin": "node bin/uyem.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "clsx": "^1.1.1",
    "html-to-text": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^8.3.2",
    "wrtc": "^0.4.7",
    "ws": "^8.7.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/html-to-text": "^8.1.0",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.39",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "airbnb": "^0.0.2",
    "concurrently": "^7.2.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "microbundle": "^0.15.0",
    "prettier": "^2.6.2",
    "react-scripts": "5.0.1",
    "sass": "^1.52.2",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "typescript": "^4.7.2"
  },
  "fhead": {
    "root": "packages/client",
    "repository": "https://github.com/kolserdav/uyem.git",
    "patterns": [
      ".js",
      ".ts",
      ".tsx",
      ".scss"
    ],
    "exclude": [
      "node-webrtc",
      "dist"
    ],
    "name": "Sergey Kolmiller",
    "email": "uyem.ru@gmail.com",
    "license": "BSD-2-Clause",
    "licenseText": "Binary distributions of this software include WebRTC and other third-party libraries.",
    "copyright": "kolserdav, All rights reserved (c)",
    "renewAll": false
  }
}
